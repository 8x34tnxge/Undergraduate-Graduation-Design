
@article{alidaee2009NoteIntegerProgramming,
  title = {A {{Note}} on {{Integer Programming Formulations}} of the {{Real-Time Optimal Scheduling}} and {{Flight Path Selection}} of {{UAVs}}},
  author = {Alidaee, Bahram and Wang, Haibo and Landram, Frank},
  date = {2009-07},
  journaltitle = {IEEE Transactions on Control Systems Technology},
  volume = {17},
  number = {4},
