  issn = {2509-3312},
  doi = {10.1007/s41650-018-0035-0},
  abstract = {In this paper, a cellular-connected unmanned aerial vehicle (UAV) mobile edge computing system is studied where several UAVs are associated to a terrestrial base station (TBS) for computation offloading. To compute the large amount of data bits, a part of computation task is migrated to TBS and the other part is locally handled at UAVs. Our goal is to minimize the total energy consumption of all UAVs by jointly adjusting the bit allocation, power allocation, resource partitioning as well as UAV trajectory under TBS's energy budget. For deeply comprehending the impact of multi-UAV access strategy on the system performance, four access schemes in the uplink transmission is considered, i.e., time division multiple access, orthogonal frequency division multiple access, one-by-one access and non-orthogonal multiple access. The involved problems under different access schemes are all formulated in non-convex forms, which are difficult to be tackled optimally. To solve this class of problem, the successive convex approximation technique is employed to obtain the suboptimal solutions. The numerical results show that the proposed scheme save significant energy consumption compared with the benchmark schemes.},
  eventtitle = {Journal of {{Communications}} and {{Information Networks}}},
  keywords = {bit allocation,Cloud computing,Computational modeling,Edge computing,Energy consumption,energy minimization,mobile edge computing,power allocation,resource partitioning,Task analysis,UAV trajectory,Unmanned aerial vehicles,Uplink},
  file = {/home/zhuying/Zotero/storage/E4CD7USL/Hua et al_2018_Energy Optimization for Cellular-Connected Multi-UAV Mobile Edge Computing.pdf;/home/zhuying/Zotero/storage/VTML6NQ5/8932187.html}
