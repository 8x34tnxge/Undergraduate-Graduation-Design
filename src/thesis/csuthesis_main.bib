  file = {/home/zhuying/Zotero/storage/SPNNMCBS/Karaman_Frazzoli_2011_Linear temporal logic vehicle routing with applications to multi-UAV mission.pdf;/home/zhuying/Zotero/storage/RWUANCH9/rnc.html}
